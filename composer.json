{
    "name": "icicleio/log",
    "description": "Asynchronous logging interface for Icicle.",
    "keywords": [
        "log",
        "asynchronous",
        "async"
    ],
    "homepage": "http://icicle.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "^2.0",
        "icicleio/stream": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Icicle\\Log\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\Log\\": "tests"
        }
    }
}
